<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">

    <!-- Center color of FAB -->
    <solid android:color="#03031B" />

    <!-- Optional border/stroke -->
    <stroke
        android:width="1dp"
        android:color="#C1A03E" />

    <!-- Size set same as FAB -->
